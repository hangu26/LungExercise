<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/border_result_breathing">

    <TextView
        android:id="@+id/tx_result_breathing"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/margin_top_tx_result_breathing_dialog"
        android:fontFamily="@font/pretendard_semibold"
        android:text="@string/tx_result_breathing"
        android:textColor="@color/black"
        android:textSize="@dimen/size_tx_result_breathing"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linear_user_time"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_side_linear_result_time"
        android:layout_marginTop="@dimen/margin_top_linear_result_time"
        android:layout_marginEnd="@dimen/margin_side_linear_result_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tx_result_breathing">

        <TextView
            android:id="@+id/tx_user_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:fontFamily="@font/pretendard_semibold"
            android:text="@string/tx_user_breathing_time"
            android:textColor="@color/appBar_title_02"
            android:textSize="@dimen/size_tx_result_breathing"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="@dimen/width_constraint_result_time_dialog"
            android:layout_height="@dimen/height_constraint_result_time_dialog"
            android:layout_marginStart="@dimen/margint_start_constraint_result_time_dialog"
            android:background="@drawable/border_constraint_breath"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/tx_user_time"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tx_user_time_data"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_regular"
                android:text="10초"
                android:textSize="@dimen/size_tx_result_breathing"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linear_normal_time"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_side_linear_result_time"
        android:layout_marginTop="@dimen/margin_top_linear_result_time"
        android:layout_marginEnd="@dimen/margin_side_linear_result_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linear_user_time">

        <TextView
            android:id="@+id/tx_normal_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:fontFamily="@font/pretendard_semibold"
            android:text="@string/tx_normal_time"
            android:textColor="@color/black"
            android:textSize="@dimen/size_tx_result_breathing"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="@dimen/width_constraint_result_time_dialog"
            android:layout_height="@dimen/height_constraint_result_time_dialog"
            android:background="@drawable/border_constraint_breath"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tx_normal_time_data"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_regular"
                android:text="10초"
                android:textSize="@dimen/size_tx_result_breathing"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linear_achievement_rate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/margin_side_linear_result_time"
        android:layout_marginTop="@dimen/margin_top_linear_result_time"
        android:layout_marginEnd="@dimen/margin_side_linear_result_time"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linear_normal_time">

        <TextView
            android:id="@+id/tx_achievement_rate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:fontFamily="@font/pretendard_semibold"
            android:text="@string/tx_achievement_rate"
            android:textColor="@color/black"
            android:textSize="@dimen/size_tx_result_breathing"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="@dimen/width_constraint_result_time_dialog"
            android:layout_height="@dimen/height_constraint_result_time_dialog"
            android:background="@drawable/border_constraint_breath"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tx_percent_data"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_regular"
                android:text="88%"
                android:textSize="@dimen/size_tx_result_breathing"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/btn_close"
        android:layout_width="@dimen/width_btn_close_in_dialog"
        android:layout_height="@dimen/height_btn_close_in_dialog"
        android:layout_marginTop="@dimen/margin_top_btn_close_in_dialog"
        android:layout_marginBottom="@dimen/margin_bottom_btn_close_in_dialog"
        android:background="@drawable/border_btn_start_breathing"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linear_achievement_rate">

        <TextView
            android:id="@+id/tx_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_bold"
            android:gravity="center"
            android:text="@string/tx_close"
            android:textColor="@color/white"
            android:textSize="@dimen/size_tx_btn_next"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>